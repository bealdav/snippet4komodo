snippet view3
	<record id="view_${1:model}_form" model="ir.ui.view">
	    <field name="name">$2.form</field>
	    <field name="model">${2:Model}</field>
	    <field name="inherit_id" ref="${3:xmlid}"/>
	    <field name="priority" eval="8"/>
	    <field name="arch" type="xml">
	        <xpath expr="//notebook" position="inside">
	            <page string="${4:text}">
	                <field name="${5:name}"/>
	            </page>
	        </xpath>
	        <field name="name" position="before">
	            <field name="${5:name}"/>
	        </field>
	        <xpath expr="//field[@name='field']" position="before">
	            <field name="${5:name}"/>
	        </xpath>
	        <xpath expr="//label[@for='is_operation']" position="attributes">
	           <attribute name="groups">base_instance.group_abilis_admin</attribute>
	        </xpath>
	        <field name="f" position="attributes">
	            <attribute name="attrs">{'readonly': [('state','=','done')]}</attribute>
	            <attribute name="colors">red:global_load>0 and used==False;grey:used==False</attribute>
	        </field>
	        <field name="f" position="attributes">
	            <attribute name="context">{'ff': id}</attribute>
	        </field>
	    </field>
	</record>
snippet act_window
	<act_window id="act_${1:ir_model_data}"
	    name="${2:Name}"
	    res_model="${3:destination_model}"
	    src_model="${4:source_model}"
	    domain="[('${5:field}', '${6:operator}', ${7:active_id})]"/>
snippet space
	&#160;
snippet xpath
	<xpath expr="//field[@${1:name}='${2:field}']" position="after${4:before/replace/inside}">
	    <field name="${3:name}"/>
	</xpath>
snippet xpath_attribute
	<xpath expr="//field[@${1:name}='${2:field}']" position="attributes">
	    <attribute name="${3:required}">${4:1}</attribute>
	</xpath>
